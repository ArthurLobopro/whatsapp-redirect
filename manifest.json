{
    "manifest_version": 3,
    "name": "WhatsApp Web Redirect",
    "version": "1.0.0",
    
    "description": "Redireciona links WhatsApp Web para o formato wa.me",
    "permissions": [
      "webNavigation",
      "tabs"
    ],
    "host_permissions": [
       "https://web.whatsapp.com/*"
    ],
    "icons": {
        "48": "icon.png"
    },
    "background": {
      "service_worker": "./dist/background.js"
    }
  }
  